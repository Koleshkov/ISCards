        <Border StrokeThickness="1"
                Stroke="{StaticResource Gray500}"
                BackgroundColor="Transparent"
                HeightRequest="2"
                StrokeShape="RoundRectangle"
                Padding="5,0,5,0">
        </Border>
    </StackLayout>
</ContentView>
